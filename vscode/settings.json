{
  "workbench.tree.renderIndentGuides": "none",
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "FiraCode Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.fontWeight": "300",
  "editor.autoIndent": "full",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.semanticHighlighting.enabled": false,
  "editor.suggestSelection": "first",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "workbench.editor.wrapTabs": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "php.suggest.basic": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "eslint.format.enable": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.cursorStyle": "line",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[php]": {
    "editor.defaultFormatter": "kokororin.vscode-phpfmt"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "conventionalCommits.autoCommit": false,
  "files.associations": {
    "*.edge": "html",
    "*.html": "twig",
    "*.inc": "php",
    "*.module": "php",
    "*.install": "php",
    "*.theme": "php",
    "*.profile": "php",
    "*.tpl.php": "php",
    "*.test": "php",
    "*.php": "php",
    "*.info": "ini"
  },
  "emmet.includeLanguages": {
    "edge": "html",
    "twig": "html"
  },
  "json.schemas": [],
  "editor.acceptSuggestionOnEnter": "smart",
  "terminal.integrated.gpuAcceleration": "off",
  "workbench.startupEditor": "none",
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "workbench.editorAssociations": {
    "*.xml": "default"
  },
  "editor.accessibilitySupport": "off",
  "[python]": {
    "editor.formatOnType": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "svelte.enable-ts-plugin": true,
  "workbench.editor.empty.hint": "hidden",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.hideUnchangedRegions.enabled": true,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.stickyScroll.enabled": false,
  "[mdx]": {
    "editor.defaultFormatter": "unifiedjs.vscode-mdx"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.systemColorTheme": "dark",
  "react-native-tools.networkInspector.consoleLogsColorTheme": "Dark",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "terminal.integrated.defaultProfile.osx": "zsh (Homebrew)",
  "telemetry.telemetryLevel": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "breadcrumbs.enabled": false,
  "conventionalCommits.showNewVersionNotes": false,
  "cSpell.userWords": ["favicons", "Tiago"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "eyecons",
  "workbench.colorTheme": "Default Dark Modern",
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[twig]": {
    "editor.defaultFormatter": "mblode.twig-language-2"
  },
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "explorer.confirmPasteNative": false,
  "trae.tab.enableAutoImport": true,
  "trae.tab.enableAutoRename": true
}
